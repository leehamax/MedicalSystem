<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.model_nodeMapper">
    <select id="findById" parameterType="int" resultType="example.pojo.User">
        SELECT * FROM model_node WHERE node_id=#{node_id}
    </select>
    <insert id="insertModel_node" parameterType="example.pojo.model_node" useGeneratedKeys="true"  >
        INSERT INTO user VALUES (#{model_id},#{node_id},#{question_content},#{question_type},#{answer_type})
    </insert>
    <select id="findNextQues" parameterType="example.pojo.model_node" resultType="example.pojo.model_node">
        SELECT * from mode_node WHERE mode_id in(SELECT mode_id from  )
    </select>

</mapper>